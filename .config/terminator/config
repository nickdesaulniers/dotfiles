[global_config]
[keybindings]
  cycle_prev = <Alt>bracketleft
  cycle_next = <Alt>bracketright
  zoom_in = <Alt>plus
  close_window = <Alt>Q
  zoom_out = <Alt>minus
  new_window = <Alt>N
  split_horiz = <Alt><Shift>D
  full_screen = <Alt>Return
  zoom_normal = <Alt>0
  move_tab_right = <Super><Shift>rightarrow
  close_term = <Alt>W
  split_vert = <Alt>D
  new_tab = <Alt>T
  move_tab_left = <Super><Shift>leftarrow
  reset = <Alt>R
  go_left = <Super>leftarrow
  go_right = <Super>rightarrow
[profiles]
  [[default]]
    use_system_font = False
    font = Ubuntu Mono 14
    copy_on_selection = True
    scrollback_infinite = True
    scrollbar_position = disabled
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
