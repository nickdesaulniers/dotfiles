[global_config]
[keybindings]
  zoom_in = <Alt>plus
  zoom_out = <Alt>minus
  zoom_normal = <Alt>0
  new_tab = <Alt>T
  cycle_next = <Alt>bracketright
  cycle_prev = <Alt>bracketleft
  go_left = <Primary>Left
  go_right = <Primary>Right
  split_horiz = <Alt><Shift>D
  split_vert = <Alt>D
  close_term = <Alt>W
  copy = <Alt>C
  paste = <Alt>V
  close_window = <Alt>Q
  move_tab_right = <Super><Shift>rightarrow
  move_tab_left = <Super><Shift>leftarrow
  full_screen = <Alt>Return
  reset = <Alt>R
  new_window = <Alt>N
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    font = Monospace Bold 16
    scrollbar_position = hidden
    scrollback_infinite = True
    use_system_font = False
    copy_on_selection = True
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
