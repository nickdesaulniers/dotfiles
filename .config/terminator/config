[global_config]
[keybindings]
  close_term = <Alt>W
  close_window = <Alt>Q
  copy = <Alt>C
  cycle_next = <Alt>bracketright
  cycle_prev = <Alt>bracketleft
  full_screen = <Alt>Return
  go_left = <Super>leftarrow
  go_right = <Super>rightarrow
  move_tab_left = <Super><Shift>leftarrow
  move_tab_right = <Super><Shift>rightarrow
  new_tab = <Alt>T
  new_window = <Alt>N
  paste = <Alt>V
  reset = <Alt>R
  split_horiz = <Alt><Shift>D
  split_vert = <Alt>D
  zoom_in = <Alt>plus
  zoom_normal = <Alt>0
  zoom_out = <Alt>minus
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    copy_on_selection = True
    cursor_color = "#aaaaaa"
    font = Monospace Bold 16
    scrollback_infinite = True
    scrollbar_position = hidden
    use_system_font = False
